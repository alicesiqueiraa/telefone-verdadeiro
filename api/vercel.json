{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "OPTIONS"
        ],
        "headers" : {
          "Access-Control-Allow-Origin" : "https://telefone-verdadeiro-frontend.vercel.app"
        }
      }
    ]
  }